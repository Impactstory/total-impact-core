{
    ###################################################################
    # GITHUB PROVIDER CONFIGURATION
    ###################################################################
    
    # Provider Identifier - used as the "id" of any resulting metrics
    # object
    #
    "id" : "github",
    
    # Namespaces supported by this provider
    #
    "supported_namespaces" : ["url"],
    
    # Mendeley API configuration for retrieving metrics
    #
    
    "aliases" : {
        "url" : ""
    },
    
    "member_items" : {
        "querytype" : {
            "github_user": {
                "url": "https://api.github.com/users/%s/repos"
            },
            "github_org":{
                "url": "https://api.github.com/orgs/%s/repos"
            }
        }
    }

}